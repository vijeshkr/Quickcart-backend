{
    "version": 2,
    "name": "QuickCart",
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node",
        "config": {
          "functions": {
            "api/*.js": {
              "timeout": 30 
            }
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods" : ["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers" : {
            "Access-Control-Allow-Origin" : "https://quickcart-frontend.vercel.app"
        }
      }
    ]
  }
  