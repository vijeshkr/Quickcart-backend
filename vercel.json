{
    "version": 2,
    "name": "QuickCart",
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node",
        "config": {
          "functions": {
            "api/*.js": {
              "timeout": 30 
            }
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  